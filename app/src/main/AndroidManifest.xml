<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mynetdiarytest">

    <application
        android:name=".MyNetDiaryApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyNetDiaryTest">

        <activity
            android:name=".screen.list.ClientsListActivity"
            android:label="@string/toolbar_main"
            android:theme="@style/Theme.MyNetDiaryTest.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".screen.edit.EditActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyNetDiaryTest.NoActionBar" />
    </application>
</manifest>